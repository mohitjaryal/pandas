   "source": [
    "import pandas as pd\n",
    "\n",
    "df1 = pd.DataFrame({'A': [1, 2]})\n",
    "df2 = pd.DataFrame({'B': ['x', 'y', 'z']})\n",
    "\n",
    "result = pd.merge(df1, df2, how='cross')\n",
    "print(result)\n"
   ]
